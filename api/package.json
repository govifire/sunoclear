{
  "name": "api",
  "version": "0.2.1",
  "private": true,
  "engines": {
    "node": "16.x.x"
  },
  "scripts": {
    "gcp-build": "npm run build",
    "build": "rm -rf build && tsc",
    "start": "node --experimental-specifier-resolution=node build/index.js",
    "dev": "nodemon",
    "prescript": "yarn run build && jq '. + { \"main\": \"./google-cloud-functions-index.js\" }' package.json | jq 'del(.scripts)' > build/package.json",
    "script": "sh -c 'functions-framework --target=${0} --source build'",
    "eslint": "eslint src/ --ext .ts --cache",
    "eslint:fix": "eslint --fix src/ --ext .ts --cache",
    "generate:swagger": "ts-node --esm --files src/index.ts --swagger",
    "typeorm": "ts-node --files --esm ./node_modules/typeorm/cli.js",
    "typeorm:migration:gen": "yarn run typeorm migration:generate -d src/config/PostgresDataSource.ts",
    "typeorm:migration:run": "yarn run typeorm migration:run -d src/config/PostgresDataSource.ts",
    "typeorm:migration:revert": "yarn run typeorm migration:revert -d src/config/PostgresDataSource.ts",
    "seed:generate": "yarn run typeorm migration:generate -d src/config/SeedDataSource.ts",
    "seed:run": "yarn run typeorm migration:run -d src/config/SeedDataSource.ts"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/multipart": "^7.4.0",
    "@fastify/swagger": "^8.3.0",
    "@google-cloud/secret-manager": "^4.2.0",
    "@google-cloud/storage": "^6.9.1",
    "@sentry/node": "^7.33.0",
    "@sinclair/typebox": "^0.25.21",
    "@trycourier/courier": "^4.1.0",
    "axios": "^1.2.4",
    "axios-retry": "^3.4.0",
    "dayjs": "^1.11.7",
    "fastify": "^4.12.0",
    "googleapis": "^110.0.0",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.6.0",
    "query-string": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "tmp": "^0.2.1",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/html-pdf": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/parser": "^5.49.0",
    "ajv": "^8.12.0",
    "eslint": "^8.32.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.9",
    "p-limit": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "type": "module"
}
